rker\n                  v-for=\"(marker, index) in markers\"\n                  :key=\"index\"\n                  ref=\"markersRef\"\n                  :lat-lng=\"marker\"></l-marker>\n\n              </l-map>\n            </client-only>\n          </div>\n\n          <template v-if=\"spot\">\n            <spot class=\"spot\"></spot>\n          </template>\n\n        </div>\n\n      <!-- ----------- -->\n    </ion-content>\n  </ion-page>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\nimport { IonPage, IonHeader, IonToolbar, IonTitle, IonContent } from '@ionic/vue';\nimport ExploreContainer from '@/components/ExploreContainer.vue';\n\nexport default defineComponent({\n  name: 'Tab2Page',\n  components: { ExploreContainer, IonHeader, IonToolbar, IonTitle, IonContent, IonPage },\n  async created() {\n    const {data: events, error} = await this.$supabase\n      .from('events')\n      .select('coordinates')\n    this.populate_markers_array(events)\n    this.loaded = true\n    // console.log(this.markers1);\n    // this.cut = this.markers1.replace(\"\", \"\");\n  },\n  data: () => ({\n    spot: false,\n    zoomControl: false,\n    loaded: false,\n    markers: [],\n  }),\n  methods: {\n    greet: function (event) {\n      this.spot = true;\n    },\n    populate_markers_array: function (marker_objects) {\n      let markersArr = []\n      marker_objects.forEach(element => {\n        let coordsArr = element.coordinates.split(',')\n        markersArr.push([parseFloat(coordsArr[0]), parseFloat(coordsArr[1])])\n      });\n      this.markers = markersArr\n\n      // simple storage update\n      // var storage = localStorage.getItem('myCat');\n      // let newData = storage + ',' + 'Murksi'\n      // window.localStorage.setItem('myCat', newData);\n      // console.log(storage)\n    }\n  }\n});\n</script>\n\n<style>\n.overlay {\n  position: absolute;\n  z-index: 3000;\n  margin-top: 20px;\n  left: 50%;\n  top: 20px;\n  transform: translate(-50%, -50%);\n  width: 90vw;\n}\n\n.margin {\n  padding-left: 10px;\n  padding-right: 10px;\n}\n\n.spot {\n  margin-top: -800px;\n  z-index: 3000;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}