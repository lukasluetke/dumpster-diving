{"ast":null,"code":"import { defineComponent } from 'vue';\nimport { IonPage, IonHeader, IonToolbar, IonTitle, IonContent } from '@ionic/vue';\nimport ExploreContainer from '@/components/ExploreContainer.vue';\nexport default defineComponent({\n  name: 'Tab2Page',\n  components: {\n    ExploreContainer,\n    IonHeader,\n    IonToolbar,\n    IonTitle,\n    IonContent,\n    IonPage\n  },\n\n  async created() {\n    const {\n      data: events,\n      error\n    } = await this.$supabase.from('events').select('coordinates');\n    this.populate_markers_array(events);\n    this.loaded = true; // console.log(this.markers1);\n    // this.cut = this.markers1.replace(\"\", \"\");\n  },\n\n  data: () => ({\n    spot: false,\n    zoomControl: false,\n    loaded: false,\n    markers: []\n  }),\n  methods: {\n    greet: function (event) {\n      this.spot = true;\n    },\n    populate_markers_array: function (marker_objects) {\n      let markersArr = [];\n      marker_objects.forEach(element => {\n        let coordsArr = element.coordinates.split(',');\n        markersArr.push([parseFloat(coordsArr[0]), parseFloat(coordsArr[1])]);\n      });\n      this.markers = markersArr; // simple storage update\n      // var storage = localStorage.getItem('myCat');\n      // let newData = storage + ',' + 'Murksi'\n      // window.localStorage.setItem('myCat', newData);\n      // console.log(storage)\n    }\n  }\n});","map":{"version":3,"mappings":"AAkDA,SAASA,eAAT,QAAgC,KAAhC;AACA,SAASC,OAAT,EAAkBC,SAAlB,EAA6BC,UAA7B,EAAyCC,QAAzC,EAAmDC,UAAnD,QAAqE,YAArE;AACA,OAAOC,gBAAP,MAA6B,mCAA7B;AAEA,eAAeN,eAAe,CAAC;AAC7BO,MAAI,EAAE,UADuB;AAE7BC,YAAU,EAAE;AAAEF,oBAAF;AAAoBJ,aAApB;AAA+BC,cAA/B;AAA2CC,YAA3C;AAAqDC,cAArD;AAAiEJ;AAAjE,GAFiB;;AAG7B,QAAMQ,OAAN,GAAgB;AACd,UAAM;AAACC,UAAI,EAAEC,MAAP;AAAeC;AAAf,QAAwB,MAAM,KAAKC,SAAL,CACjCC,IADiC,CAC5B,QAD4B,EAEjCC,MAFiC,CAE1B,aAF0B,CAApC;AAGA,SAAKC,sBAAL,CAA4BL,MAA5B;AACA,SAAKM,MAAL,GAAc,IAAd,CALc,CAMd;AACA;AACD,GAX4B;;AAY7BP,MAAI,EAAE,OAAO;AACXQ,QAAI,EAAE,KADK;